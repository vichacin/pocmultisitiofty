  "image#cta": {
    "props": {
      "blockClass": "cover",
      "width": "100%",
      "height": 400,
      "src": "https://tiendadevi.vteximg.com.br/arquivos/home1.jpg"
    }
  },

  "link#cta": {
    "props": {
      "displayMode": "button",
      "buttonProps": {
        "variant": "primary",
        "size": "large"
      },
      "href": "/exm-api-home---deco-victoria-chacin",
      "label": "Explore"
    }
  },

